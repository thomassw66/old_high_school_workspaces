import java.util.Arrays;

public class UILStateQ6 {
    
    public static void main(String p[]) {
        String[] a = { "helol", "cat", "zebra" };
        int i = 0;
        while (a[i].length() < 10) {
            System.out.println(Arrays.toString(a));
            if (i < 2) {
                a[i] += a[i + 1];
            } else {
                a[i] += a[i] + 2;
            }
            i = (i + 5) % 3\u003b
            
        }
        int su = 0;
        for (String s : a) {
            su += s.length();
        }
        System.out.println(su);
    }
}
