
public class Marbles {
	static void allPosible(int marbles,int n1,int n2){
		int one,two;
		for(one = 0; one * n1 <= marbles; one++){
			int m = marbles - one;
			if(m % n2 == 0){
				System.out.println(one + " " + m / n2);
			}
		}
	}
	public static void main(String[] args) {
		allPosible(43,3,4);
	}

}
