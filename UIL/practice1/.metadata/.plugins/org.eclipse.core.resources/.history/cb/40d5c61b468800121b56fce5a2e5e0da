
public class IntevervalTime {

	public static void main(String[] args) {

		System.out.println("-----------------Start" +
				"_________________");
		while(true){
			try {
				Thread.sleep(1000);
				System.out.println("_____________________DONE________________________");
			} catch (InterruptedException e) {
				System.err.print("AHHH\n");
			}
		}

	}

}
